{
  "name": "exman",
  "version": "1.0.1",
  "description": "Javascript wrapper around Adobe's Extension Manager command line tool",
  "main": "src/index.js",
  "scripts": {
    "unzip_mac": "unzip -o bin/ExManCmd_mac.zip -d bin/ExManCmd_mac/",
    "unzip_win": "unzip -o bin/ExManCmd_win.zip -d bin/ExManCmd_win/",
    "postinstall": "npm run unzip_mac && npm run unzip_win"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bhstahl/exman.git"
  },
  "keywords": [
    "adobe",
    "exman",
    "exmancmd",
    "extension"
  ],
  "author": "Ben Stahl <bhstahl@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bhstahl/exman/issues"
  },
  "homepage": "https://github.com/bhstahl/exman#readme"
}
